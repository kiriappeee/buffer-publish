{
  "name": "@bufferapp/publish",
  "private": true,
  "version": "0.0.1",
  "description": "üç©",
  "scripts": {
    "bootstrap": "yarn",
    "clean": "lerna clean",
    "test": "if [ -n \"$CI\" ]; then yarn && jest --runInBand; else jest; fi",
    "test-update": "jest -u",
    "init": "yarn",
    "start": "cd ./packages/server/ && yarn run start",
    "storybook": "start-storybook -p 9001",
    "publish": "lerna publish --exact",
    "publish-beta": "lerna publish --npm-tag=beta --exact",
    "watch": "webpack-dev-server --config ./packages/server/webpack.config.dev.js --colors --display-error-details",
    "build": "webpack --config ./packages/server/webpack.config.prod.js --output-path=`pwd`",
    "updated": "lerna updated",
    "rebuild-grpc": "docker exec -it bufferdev_publish_1 bash -c 'npm rebuild grpc --target_arch=x64 --target_platform=linux --target_libc=glibc'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bufferapp/project-donut.git"
  },
  "author": "Harrison Harnisch <hharnisc@gmail.com> (http://hharnisc.github.io)",
  "bugs": {
    "url": "https://github.com/bufferapp/project-donut/issues"
  },
  "homepage": "https://github.com/bufferapp/project-donut#readme",
  "devDependencies": {
    "@bufferapp/components": "2.3.4-beta.1",
