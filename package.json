  "jest": {
    "transformIgnorePatterns": [
      "/node_modules(?!\/@bufferapp\/performance-tracking)(?!\/@bufferapp\/async-data-fetch)(?!\/@bufferapp\/components)(?!\/@bufferapp\/web-components)(?!\/@bufferapp\/composer)/"
    ],
    "verbose": true,
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/jest-raf-shim.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupEnzyme.js",
    "globals": {
      "__PACKAGES__": "../packages"
    },
    "testURL": "https://publish.local.buffer.com"
  },
